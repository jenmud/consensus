{
	// Use IntelliSense to learn about possible attributes.
	// Hover to view descriptions of existing attributes.
	// For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
	"version": "0.2.0",
	"configurations": [
		{
			"name": "UI debugging",
			"type": "go",
			"request": "launch",
			"mode": "debug",
			"buildFlags": ["-tags", "fts5"],
			"args": ["web", "ui", "--address", "0.0.0.0:8090", "--service", "0.0.0.0:8001"],
			"program": "${workspaceFolder}/app/main.go"
		},
		{
			"type": "chrome",
			"request": "launch",
			"name": "Launch Chrome against 192.168.1.123",
			"url": "http://192.168.1.123:3000",
			"webRoot": "${workspaceFolder}",
			"port": 9222
		}
	]
}